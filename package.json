    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-image-gallery": "^1.3.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.22.1",
    "redux-persist": "^6.0.0",
    "sonner": "^1.4.0",
    "tailwind-merge": "^2.2.1",
    "uuid": "^9.0.1",
    "web-vitals": "^3.5.2",
    "zod": "^3.22.4"
