    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.4.0",
    "chart.js": "^4.4.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "dompurify": "^3.0.6",
    "firebase": "^10.14.1",
    "idb": "^8.0.0",
    "jspdf": "^2.5.1",
